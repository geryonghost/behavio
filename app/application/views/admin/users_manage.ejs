<%
    if (locals.users != null && locals.users != undefined) {
        users = locals.users
    }
%>
     <!-- Font Awesome-->
     <script src="/js/fontawesome.js" crossorigin="anonymous"></script>

Manage Users (<a href="/admin/users_new">New User</a>)

<% if (users.length > 0) { %>
    <br />
    <div class="row">
      <div class="col">
        <div class="card-body">
          <table class="table table-primary" border="1">
            <thead>
              <tr>
                <td>Name</td>
                <td></td>
              </tr>
            </thead>
            <tbody>
              <% users.forEach((user) => { %>
                <tr>
                    <td><%=user.name%></td>
                    <td>
                        <a href="/admin/users_edit?id=<%=user._id%>"><i class="fa-solid fa-pencil"></i></a>
                        <a href="/usersdelete?id=<%=user._id%>"><i class="fa-regular fa-trash-can"></i></a>
                    </td>
                </tr>
            <% }) %>
            </tbody>
            </table>
        </div>
      </div>
    </div>
<% } %>
